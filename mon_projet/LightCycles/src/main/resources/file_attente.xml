<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ScrollPane?>
<?import frontal.vues.controles.CanvasPartie?>

<VBox xmlns:fx="http://javafx.com/fxml"
	fx:controller="frontal.vues.VueFileAttente"
	styleClass="conteneur"
	id="vue-file-attente">


	<Pane styleClass="petit-espace-vertical"/>

	<HBox VBox.vgrow="ALWAYS" styleClass="conteneur">

		<Pane styleClass="petit-espace-horizontal"/>


		<CanvasPartie HBox.hgrow = "ALWAYS"
			fx:id="canvasPartie"
			id="canvas-partie">
		</CanvasPartie>

		<Pane styleClass="petit-espace-horizontal"/>

	</HBox>

	<Pane styleClass="petit-espace-horizontal"/>
	

	<Button 
		fx:id="boutonMaPage"
		text="%ouvrirPageTouche"
		styleClass="gros-bouton"
		id="bouton-ma-page">
	</Button>

	<Pane styleClass="petit-espace-vertical"/>

	
</VBox>
