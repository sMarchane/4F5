<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import frontal.vues.controles.FlexBoxTouche?>


<HBox xmlns:fx="http://javafx.com/fxml"
    fx:controller="frontal.vues.fragments.FragmentPageTouche" 
    styleClass="conteneur,fragment-page-touche">

    <Pane styleClass="grand-espace-horizontal"/>

    <FlexBoxTouche HBox.hgrow="ALWAYS" styleClass="conteneur,page-touche">

        <Label
            fx:id="labelNomTouche"
            styleClass="label-nom-touche">
        </Label>

        <Pane HBox.hgrow="ALWAYS" minHeight="20"/>

        <Button 
            fx:id="boutonOuvrirFileAttente" 
            text="%ouvrirFileAttente"
            styleClass="gros-bouton">
        </Button>

    </FlexBoxTouche>

    <Pane styleClass="grand-espace-horizontal"/>

</HBox>
