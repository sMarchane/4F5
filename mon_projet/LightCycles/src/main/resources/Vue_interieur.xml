<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Text?>

<VBox xmlns:fx="http://javafx.com/fxml"
	fx:controller="frontal.vues.VueInterieur"
	styleClass="conteneur"
	id="vue-interieur">	

	<Button text="%ouvrirRetour"
	styleClass="gros-bouton"
	id="bouton-retour">
	</Button>

	<Text
		text="%ParametreTouches"
		styleClass="titre-page"
		id="titre">
	</Text>


	<ImageView>
		<image>
			<Image url="@logo.jpg"/>
		</image>
	</ImageView>

	<Button
		fx:id="boutonFileAttente"
		text="%fileAttente"
		styleClass="gros-bouton"
		id="boutonFileAttente">
	</Button>


</VBox>
